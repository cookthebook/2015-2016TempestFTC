        <activity
            android:name=".AutonomousActivity"
            android:label="@string/title_activity_autonomous" >
        </activity>
        <activity
            android:name=".DriveActivity"
            android:label="@string/title_activity_drive" >
        </activity>
        <activity
            android:name=".MotorActivity"
            android:label="@string/title_activity_motor" >
        </activity>
        <activity
            android:name=".SensorActivity"
            android:label="@string/title_activity_sensor" >
        </activity>
    </application>

</manifest>
